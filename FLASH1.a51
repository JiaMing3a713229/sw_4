;9-2
ORG 0000H
	START:	MOV		R1,#07H
			MOV		A,#11111110B
	LEFT:	MOV	P1,A
			ACALL	DELAY
			RL	A
			DJNZ	R1,LEFT
			
			MOV		R1,#07H
			MOV		A,#01111111B
	RIGHT:	MOV		P1,A
			ACALL 	DELAY
			RR	A
			DJNZ	R1,RIGHT
			AJMP	START
	
	DELAY:	MOV	R5,#2000
	DL1:	MOV	R6,#250
	DL2:	MOV R7,#200
	DL3:	DJNZ	R7,DL3
			DJNZ	R6,DL2
			DJNZ	R5,DL1
			RET
END
	